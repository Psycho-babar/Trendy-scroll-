{
    "name": "Trendyol Scroller",
    "version": "1.0",
    "description": "Automatically scrolls the Trendyol website to the bottom and back to the top.",
    "manifest_version": 3,
    "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
    "content_scripts": [
      {
        "matches": ["*://*/*"],
        "js": ["content.js"]
      }
    ]
  }
  